@font-face {
  font-family: "阿里妈妈东方大楷 Regular";
  font-weight: 100;
  src: url("//at.alicdn.com/wf/webfont/OP6oyiyhTCM3/Sq820hAgGUec.woff2") format("woff2"),
    url("//at.alicdn.com/wf/webfont/OP6oyiyhTCM3/5OtC91pyqBl3.woff") format("woff");
  font-display: swap;
}


page {
  height: 100%;
  overflow-y: auto;
  overflow-x: hidden;
}

.container {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: space-around;
  align-items: center;
 
  flex-direction: column;
}

.background {
  width: 100%;
  height: 100%;
  position: fixed;
  background-size: 100% 100%;
  z-index: -1;
}

.head {
  font-family: "阿里妈妈东方大楷 Regular";
  font-size: 70rpx;
  color: #754E00;
  position: absolute;
  top: 10%;
}
.land{
  position: absolute;
  top: 20%;
  display: flex;
  width: 100%;
  justify-content: space-around;
  align-items: center;
  flex-direction: column;
  gap: 130rpx;
}

.two{
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex-direction: row;
  width: 100%;
}
.two image{
  width: 40%;
}
.image {
  opacity: 0;
  z-index: 999;
  display: block;
  transform: scale(0.1);  /* 初始缩小 */
  box-sizing: border-box;
  width: 450rpx;
  height: 380rpx;
}

/* .tname{
