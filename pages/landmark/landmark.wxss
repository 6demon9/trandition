} */

.red-line {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  top: 190rpx;
  width: 10rpx; /* 红线的宽度 */
  background-color: rgb(156, 84, 66);
  height: 0; /* 初始高度为0 */
  transition: height 14s; /* 动画持续时间2秒 */
}
.wh{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.tname{
  width:250rpx;
  height: 80rpx;
}